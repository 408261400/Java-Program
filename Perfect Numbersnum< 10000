public class Main {  
  
    public static void main(String[] args) {  
        int temp;  
        for (int num = 1; num < 10000; ++num) {  
            temp = 0;  
            for (int i = 1; i < num; ++i) {  
                if (num % i == 0) {  
                    temp = temp + i;  
                }  
            }  
            if (num == temp) {  
                System.out.println(num);  
            }  
        }  
    }  
}  
